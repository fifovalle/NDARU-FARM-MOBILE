{
  "expo": {
    "name": "Ndaru Farm",
    "slug": "Ndaru-Farm",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ikon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/ikonSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#447055"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/ikon.png",
        "backgroundColor": "#447055"
      },
      "package": "com.fifovalle.NdaruFarm"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/ikon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7dde86ec-2c80-444c-97e4-74b63da3706d"
      }
    }
  }
}
